#include "SysTick.h"


bool SysTick_Init (void (*funcallback)(void))
{


	SysTick->CTRL = 0x00;
	SysTick->LOAD = 12500000L - 1; // 125ms @ 100MHz
	SysTick->VAL = 0x00;
	SysTick->CTRL = SysTick_CTRL_CLKSOURCE_Msk |
	SysTick_CTRL_TICKINT_Msk |
	SysTick_CTRL_ENABLE_Msk;






}



__ISR__ SysTick_Handler (void)
{
// Your code Here
}
